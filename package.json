{
  "name": "dashborion",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "sst dev",
    "deploy": "npm run build:auth && sst deploy",
    "deploy:staging": "npm run build:auth && sst deploy --stage staging",
    "deploy:prod": "npm run build:auth && sst deploy --stage production",
    "remove": "sst remove",
    "console": "sst console",
    "types": "sst types",
    "build:auth": "cd auth && npm install && npm run build",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "cli:install": "cd cli && pip install -e .",
    "tf:init": "cd terraform && terraform init",
    "tf:plan": "cd terraform && terraform plan",
    "tf:apply": "cd terraform && terraform apply"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "sst": "^3.0.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@pulumi/aws": "^6.0.0",
    "@pulumi/pulumi": "^3.0.0",
    "@types/mime-types": "^3.0.1",
    "mime-types": "^2.1.35",
    "synced-folder": "^0.11.0"
  }
}
