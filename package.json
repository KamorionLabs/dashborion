{
  "name": "dashborion",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.27.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "deploy": "pnpm run build && sst deploy",
    "deploy:staging": "pnpm run build && sst deploy --stage staging",
    "deploy:prod": "pnpm run build && sst deploy --stage production",
    "remove": "sst remove",
    "console": "sst console",
    "types": "sst types",
    "cli:install": "cd cli && pip install -e .",
    "tf:init": "cd terraform && terraform init",
    "tf:plan": "cd terraform && terraform plan",
    "tf:apply": "cd terraform && terraform apply",
    "local:up": "docker-compose up -d",
    "local:down": "docker-compose down",
    "local:logs": "docker-compose logs -f localstack",
    "local:setup": "./scripts/setup-localstack.sh",
    "local:start": "docker-compose up -d && sleep 3 && ./scripts/setup-localstack.sh",
    "backend:install": "uv pip install -e '.[dev]'",
    "backend:dev": "cd backend && uv run flask --app dev_server run --reload --port 8080",
    "dev:full": "pnpm local:start && concurrently 'pnpm backend:dev' 'cd packages/frontend && pnpm dev'"
  },
  "devDependencies": {
    "@types/mime-types": "^3.0.1",
    "@types/node": "^20.10.0",
    "sst": "^3.0.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.965.0",
    "@aws-sdk/client-s3": "^3.965.0",
    "@aws-sdk/credential-providers": "^3.965.0",
    "@dashborion/core": "workspace:*",
    "@dashborion/sst": "workspace:*",
    "@pulumi/aws": "^6.0.0",
    "@pulumi/pulumi": "^3.0.0",
    "@pulumi/synced-folder": "^0.12.0",
    "mime-types": "^2.1.35"
  }
}
